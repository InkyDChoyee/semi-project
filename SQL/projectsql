-- 배송 테이블 생성
CREATE TABLE Delivery(
    dno             NUMBER PRIMARY KEY,
    name            VARCHAR2(30) NOT NULL,
    address         VARCHAR2(40) NOT NULL,
    detailaddress   VARCHAR2(40) NOT NULL,
    request         VARCHAR2(200),
    orderno         NUMBER NOT NULL
);
    
CREATE TABLE order(
    orderno         NUMBER PRIMARY KEY,
    orderdate       TIMESTAMP 